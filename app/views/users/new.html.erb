<%= form_for @user, url:{action: :create} do |f| %>
    <%= f.label :uid, "ユーザーID" %>
    <%= f.text_field :uid %>
    <%= f.label :password, "パスワード" %>
    <%= f.password_field :password %>
    <%= f.label :password_confirmation, 'パスワード(確認)' %>
    <%= f.password_field :password_confirmation %>
    <%= f.submit "登録" %>
<% end %>
